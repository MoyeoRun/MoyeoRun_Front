			"import CustomWebview from './common/CustomWebview';",
			"",
			"type ${1}Props = {",
			"};",
			"",
			"const ${1} = ({ }: ${1}Props) => {",
			"const webview = useRef<any>();",
			"",
			"const sendProps = () => {",
			"webview.current.postMessage(",
			"JSON.stringify({ type: '${1}', value: {  } }),",
			");",
			"};",
			"",
			"const handleEvent = (event: WebViewMessageEvent) => {",
			"const data = JSON.parse(event.nativeEvent.data);",
			"switch (data.type) {",
			"case 'logout': {",
			"onLogout();",
			"return;",
			"}",
			"}",
			"};",
			"",
			"useEffect(() => {",
			"sendProps();",
			"}, []);",
			"",
			"return (",
			"<Box flex={1}>",
			"<CustomWebview parentRef={webview} path='${1}' onLoad={sendProps} onMessage={handleEvent} />;",
